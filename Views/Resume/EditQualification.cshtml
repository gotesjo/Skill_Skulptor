@model CreateQualificationModel
@{
    ViewData["Title"] = "Redigera Färdighet";
}

<h2>@ViewData["Title"]</h2>

<form asp-controller="Resume" asp-action="EditQualification" method="post" enctype="multipart/form-data">

    <!-- Kompetens Sektion -->
    <div class="experiences-info mb-4">
        <h3>Erfarenheter</h3>
        <div class="mb-3">
            <div>
                <input type="hidden" asp-for="QID">

                <label asp-for="QName" class="form-label">Typ av kompetens</label>
                <span asp-validation-for="QName" class="text-danger"></span>
                <input asp-for="QName" class="form-control" placeholder="Typ av kompetenser" />

                <label asp-for="QDescription" class="form-label">Beskrivning</label>
                <span asp-validation-for="QDescription" class="text-danger"></span>
                <input asp-for="QDescription" class="form-control" placeholder="Beskrivning" />
            </div>
        </div>

    </div>

    <button type="submit" class="btn btn-primary">Spara Färdighet</button>
</form>